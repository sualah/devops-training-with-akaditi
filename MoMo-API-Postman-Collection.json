	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"// Pre-request script to set common headers and validate environment",
					"if (!pm.environment.get('api_key')) {",
					"    console.warn('API key not set in environment variables');",
					"    pm.environment.set('api_key', 'dev-api-key-12345');",
					"}",
					"",
					"if (!pm.environment.get('base_url')) {",
					"    console.warn('Base URL not set in environment variables');",
					"    pm.environment.set('base_url', 'http://localhost:3001');",
					"}",
					"",
					"// Set timestamp for request tracking",
					"pm.environment.set('timestamp', new Date().toISOString());"
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"exec": [
					"// Test script to validate responses and set environment variables",
					"pm.test('Response time is less than 5000ms', function () {",
					"    pm.expect(pm.response.responseTime).to.be.below(5000);",
					"});",
					"",
					"pm.test('Response has valid JSON structure', function () {",
					"    pm.response.to.have.jsonBody();",
					"});",
					"",
					"pm.test('Response contains success or error field', function () {",
					"    const jsonData = pm.response.json();",
					"    pm.expect(jsonData).to.have.property('success').or.have.property('error');",
					"});",
					"",
					"// Log response for debugging",
					"console.log('Response Status:', pm.response.status);",
					"console.log('Response Body:', pm.response.json());",
					"",
					"// Set environment variables for successful responses",
					"if (pm.response.code === 200 || pm.response.code === 201) {",
					"    const jsonData = pm.response.json();",
					"    if (jsonData.reference) {",
					"        pm.environment.set('last_reference', jsonData.reference);",
					"    }",
					"    if (jsonData.timestamp) {",
					"        pm.environment.set('last_timestamp', jsonData.timestamp);",
					"    }",
					"}"
				]
			}
		}
	],
